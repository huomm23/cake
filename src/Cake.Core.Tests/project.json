{
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "Cake.Core": { "target": "project" },
    "Cake.Testing": { "target": "project" },
    "Cake.Testing.Xunit": { "target": "project" },
    "NSubstitute": "1.10.0.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
      }
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "warningsAsErrors": true
      }
    }
  },
  "buildOptions": {
    "embed": [
      "Unit/Scripting/CodeGen/Expected/Methods/*",
      "Unit/Scripting/CodeGen/Expected/Properties/*"
    ],
    "additionalArguments": [
      "/ruleset:../Test.ruleset",
      "/additionalfile:../stylecop.json"
    ]
  },
  "frameworks": {
    "net451": {
      "dependencies": {
      },
      "frameworkAssemblies": {
        "System.Runtime": { "type": "build" }
      }
    }
  },

  "testRunner": "xunit"
}