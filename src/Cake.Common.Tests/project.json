{
  "version": "0.15.3-*",
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../Test.ruleset",
      "/additionalfile:../stylecop.json"
    ]
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "warningsAsErrors": true
      }
    }
  },
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "Cake.Core": {
      "target": "project"
    },
    "Cake.Common": {
      "target": "project"
    },
    "Cake.Testing": {
      "target": "project"
    },
    "Cake.Testing.Xunit": {
      "target": "project"
    },
    "NSubstitute": "1.10.0.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "frameworks": {
    "net451": {}
  },
  "testRunner": "xunit"
